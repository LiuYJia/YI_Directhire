
<div class="container">
            <h2><%= title %></h2>               

    <ul class="nav nav-tabs nav-justified">
        <li class="active sort"><a href="#recruit" data-toggle="tab">招聘者列表</a></li>
        <li class="sort"><a href="#seeker" data-toggle="tab">求职者列表</a></li>
        <form role="form" class="search">
                <div class="input-group">
                    <input type="text" class="form-control">
                    <button class="btn btn-info">搜索用户</button>
                </div>
        </form>
    </ul>

    <div class="tab-content">
        <div class="tab-pane fade in active" id="recruit">

            <table class="table  table-hover table-condensed usertable">
                <thead>
                    <tr>
                        <th>序号</th>
                        <th>商家</th>
                        <th>招聘需求</th>
                        <th>注册日期</th>
                        <th>年龄</th>
                        <th>用户名</th>                        
                        <th>邮箱</th>
                        <th>手机号</th>
                        <th>权限</th>
                    </tr>
                </thead>
                <tbody>
                    <% data1.forEach(function(item){ %>
                        <tr>
                            <td><%= item.r_id %></td>
                            <td><%= item.name %></td>                            
                            <td><%= item.job %></td>
                            <td><%= item.reg_date %></td>
                            <td><%= item.age %></td>
                            <td><%= item.username %></td>                            
                            <td><%= item.email %></td>
                            <td><%= item.phone %></td>
                            <td><button class="btn btn-danger">删除</button></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
               

        </div>

        <div class="tab-pane fade" id="seeker">
            <table class="table  table-hover table-condensed usertable">
                <thead>
                    <tr>
                        <th>序号</th>
                        <th>姓名</th>
                        <th>性别</th>
                        <th>求职方向</th>
                        <th>注册日期</th>
                        <th>年龄</th>
                        <th>用户名</th>
                        <th>手机号</th>
                        <th>邮箱</th>
                        <th>权限</th>
                    </tr>
                </thead>
                <tbody>
                    <% data2.forEach(function(item){ %>
                        <tr>
                            <td><%= item.s_id %></td>
                            <td><%= item.name %></td>
                            <td><%= item.sex %></td>
                            <td><%= item.job %></td>
                            <td><%= item.reg_date %></td>
                            <td><%= item.age %></td>
                            <td><%= item.username %></td>
                            <td><%= item.phone %></td>
                            <td><%= item.email %></td>
                            <td><button class="btn btn-danger">删除</button></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>

        </div>
    </div>
                

</div>